<!DOCTYPE html>
<html lang="en">

<head>

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) : "Sophos" %></title>
  <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Sophos" %>">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'simple-sidebar', media: 'all', 'data-turbolinks-track' => true %>

  <%#= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%#= javascript_include_tag :application %>
  <%= csrf_meta_tags %>
</head>

<body>

<div id="wrapper">

  <!-- Sidebar -->
  <div id="sidebar-wrapper">
    <ul class="sidebar-nav">
      <li class="sidebar-brand">
        <a href="#">
          Home
        </a>
      </li>
      <li>
        <%=link_to "News", new_news_path%>
      </li>
      <li>
        <%=link_to "Regions", regions_path%>
      </li>
      <li>
        <%=link_to "Events", events_path%>
      </li>
      <li>
        <a href="#">Products</a>
        <%=link_to "Products",products_path %>
      </li>
       <li>
        <%=link_to "Banner",banners_path %>
        </li>

      <li>
        <%#=link_to "users",users_path %>
      </li>
    </ul>
  </div>
  <!-- /#sidebar-wrapper -->

  <!-- Page Content -->
  <div id="page-content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
            <%= yield %>

        </div>
      </div>
    </div>
  </div>
  <!-- /#page-content-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery Version 1.11.0 -->
<script src="js/jquery-1.11.0.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="js/bootstrap.min.js"></script>

<!-- Menu Toggle Script -->
<script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
</script>

</body>

</html>



